{
  "name": "webskills",
  "author": "Colamda GmbH",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "format:write": "prettier --write .",
    "linting:write": "ng lint --fix",
    "test.ci": "yarn test.ts-utils && yarn test.logging && yarn test.ng-utils && yarn test.ng-components",
    "test.ts-utils": "jest --config ./packages/ts-utils/jest.config.ts --reporters=jest-junit --reporters=default",
    "test.logging": "jest --config ./packages/logging/jest.config.ts --reporters=jest-junit --reporters=default",
    "test.ng-utils": "jest --config ./packages/ng-utils/jest.config.ts --reporters=jest-junit --reporters=default",
    "test.ng-components": "jest --config ./packages/ng-components/jest.config.ts --reporters=jest-junit --reporters=default",
    "build": "yarn build.ts-utils && yarn build.logging && yarn build.ng-utils && yarn build.ng-components",
    "build.ts-utils": "ng build ts-utils",
    "build.logging": "ng build logging",
    "build.ng-utils": "ng build ng-utils",
    "build.ng-components": "ng build ng-components",
    "build:watch.ng-components": "ng build ng-components --watch",
    "serve:demo": "ng serve webskills-demo --port 4202",
    "lint": "ng lint",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.0.3",
    "@angular/cdk": "17.0.0",
    "@angular/common": "17.0.3",
    "@angular/compiler": "17.0.3",
    "@angular/core": "17.0.3",
    "@angular/elements": "^17.0.3",
    "@angular/forms": "17.0.3",
    "@angular/localize": "17.0.3",
    "@angular/material": "17.0.0",
    "@angular/platform-browser": "17.0.3",
    "@angular/platform-browser-dynamic": "17.0.3",
    "@angular/router": "17.0.3",
    "fast-deep-equal": "3.1.3",
    "rfdc": "1.3.0",
    "rxjs": "7.8.1",
    "tslib": "^2.0.0",
    "vega": "5.26.1",
    "vega-embed": "6.23.0",
    "vega-lite": "5.16.3",
    "zone.js": "0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.0.1",
    "@angular-devkit/core": "17.0.1",
    "@angular-devkit/schematics": "17.0.1",
    "@angular-eslint/builder": "17.1.0",
    "@angular-eslint/eslint-plugin": "17.1.0",
    "@angular-eslint/eslint-plugin-template": "17.1.0",
    "@angular-eslint/schematics": "17.1.0",
    "@angular-eslint/template-parser": "17.1.0",
    "@angular/cli": "17.0.1",
    "@angular/compiler-cli": "17.0.3",
    "@angular/language-service": "17.0.3",
    "@schematics/angular": "17.0.1",
    "@types/jest": "29.5.8",
    "@types/node": "20.9.0",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.53.0",
    "husky": "^9.0.11",
    "jest": "29.7.0",
    "jest-junit": "^13.2.0",
    "jest-preset-angular": "13.1.4",
    "ng-packagr": "17.0.0",
    "postcss": "^8.4.5",
    "postcss-import": "15.1.0",
    "postcss-preset-env": "9.3.0",
    "postcss-url": "10.1.3",
    "prettier": "3.1.0",
    "tailwindcss": "3.3.5",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}
